# For each component package name specific content may be defined:
# - runtime content,
# - content provided for development,
# - debugging content, 
# - ...

COMP_PACKAGES=(daemonize daemonize_demos)

COMP_PACKAGE_daemonize=(runtime devel)

COMP_PACKAGE_daemonize_runtime=("lib")

COMP_PACKAGE_daemonize_devel=("include" "lib")

COMP_PACKAGE_daemonize_demos=(runtime)

COMP_PACKAGE_daemonize_demos_runtime=("bin/hello_daemonize")

